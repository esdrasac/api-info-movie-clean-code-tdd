{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "indent": [
          "error",
          2,
          {
              "SwitchCase": 1
          }
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ],
      "eol-last": "error",
      "no-undef": "off",
      "no-case-declarations": "off",
      "no-unused-vars": "off"
  }
}